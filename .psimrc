// .simrc file to go in the home directory of all GENESIS users
//===========================================================================
//                         environment variables
//===========================================================================
//      SIMPATH - path to search for scripts (includes the startup path)
//      SIMNOTES - default file for simulator notes
//===========================================================================
setenv SIMPATH	. /home/mcdaniel/Software/genesis-2.4/pgenesis/startup \
		/home/mcdaniel/Software/genesis-2.4/genesis/startup		\
		/home/mcdaniel/Software/genesis-2.4/genesis/Scripts/neurokit   \
		/home/mcdaniel/Software/genesis-2.4/genesis/Scripts/neurokit/prototypes
setenv SIMNOTES {getenv HOME}/.notes
setenv GENESIS_HELP /home/mcdaniel/Software/genesis-2.4/genesis/Doc

//===========================================================================
//                             set up tables 
//===========================================================================
pschedule			// default simulation schedule

// See /home/mcdaniel/Software/genesis-2.4/genesis/startup/escapelist.g for macro definitions.  These allow
// you to use cursor keys for the recall and editting of previous commands
include          escapelist      // setup macro keys for DEC and SUN keyboards
